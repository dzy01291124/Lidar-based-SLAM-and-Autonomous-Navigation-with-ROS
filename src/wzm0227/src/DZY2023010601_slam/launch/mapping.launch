<launch>
    <param name="use_sim_time" value="false"/>
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
      <rosparam file="$(find wzm0227_mapping)/param/gmapping_params.yaml" command="load" />
    </node>
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find wzm0227_mapping)/rviz/slam.rviz" />
</launch>
